{
  "name": "kset",
  "version": "1.0.0",
  "description": "Korea Stock Exchange Trading Library - Korea's Standard Trading Interface. Unified API for all Korean securities brokers including Kiwoom, Korea Investment, and more.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "browser": "dist/browser/kset.min.js",
  "scripts": {
    "build": "npm run clean && npm run build:node && npm run build:browser",
    "build:node": "tsc -p tsconfig.prod.json",
    "build:browser": "rollup -c",
    "build:umd": "webpack --mode=production",
    "build:prod": "node scripts/build-production.js",
    "build:ci": "npm run validate && npm run build:prod && npm run test:e2e",
    "build:watch": "npm run build:node -- --watch",
    "build:dev": "webpack --mode=development --watch",
    "dev": "tsx src/index.ts",
    "dev:server": "webpack serve --mode=development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:integration": "jest --config jest.integration.config.js",
    "test:performance": "node scripts/performance-test.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "docs:api": "typedoc src/index.ts --out docs/api",
    "docs:serve": "http-server docs -p 8080",
    "clean": "rimraf dist coverage docs/api",
    "prepublishOnly": "npm run validate && npm run build && npm run test && npm run docs",
    "validate": "npm run typecheck && npm run lint && npm run security-check && npm run size-check",
    "security-check": "npm audit --audit-level=moderate",
    "size-check": "bundlesize",
    "analyze": "webpack-bundle-analyzer dist/browser/*.js",
    "benchmark": "node scripts/benchmark.js",
    "start:demo": "tsx examples/demo.ts",
    "start:realtime": "tsx examples/real-time-demo.ts",
    "start:advanced": "tsx examples/advanced-trading-demo.ts",
    "start:research": "tsx examples/research-demo.ts",
    "docker:build": "docker build -t kset .",
    "docker:run": "docker run -p 3000:3000 kset",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "plugin:create": "node scripts/create-plugin.js",
    "plugin:test": "npm run test -- --testPathPattern=plugins",
    "release:dry": "semantic-release --dry-run",
    "release": "semantic-release",
    "release:patch": "node scripts/release-manager.js release patch",
    "release:minor": "node scripts/release-manager.js release minor",
    "release:major": "node scripts/release-manager.js release major",
    "release:validate": "node scripts/release-manager.js validate",
    "release:rollback": "node scripts/release-manager.js rollback",
    "postinstall": "node scripts/postinstall.js"
  },
  "keywords": [
    "korean",
    "securities",
    "trading",
    "stock",
    "kospi",
    "kosdaq",
    "konex",
    "krx",
    "kiwoom",
    "korea-investment",
    "키움증권",
    "한국투자증권",
    "api",
    "financial",
    "typescript",
    "standard",
    "real-time",
    "algorithm-trading",
    "websocket",
    "korea-stock-exchange",
    "korean-market",
    "broker-api",
    "trading-bot",
    "quantitative-trading"
  ],
  "author": {
    "name": "KSET Team",
    "email": "team@kset.dev",
    "url": "https://kset.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kset/kset.git"
  },
  "bugs": {
    "url": "https://github.com/kset/kset/issues"
  },
  "homepage": "https://kset.dev",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/kset"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "browser": "./dist/browser/kset.min.js"
    },
    "./package.json": "./package.json",
    "./README.md": "./README.md",
    "./LICENSE": "./LICENSE"
  },
  "dependencies": {
    "ws": "^8.14.2",
    "node-fetch": "^3.3.2",
    "crypto": "^1.0.1",
    "big.js": "^6.2.1",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.43",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/ws": "^8.5.8",
    "@types/big.js": "^6.2.2",
    "@types/lodash": "^4.14.199",
    "@types/uuid": "^9.0.6",
    "@types/jest": "^29.5.6",
    "@types/inquirer": "^9.0.6",
    "@types/ora": "^3.2.0",
    "@types/chalk": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-replace": "^5.0.5",
    "rollup": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript-transform-paths": "^3.4.6",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-dev-server": "^4.15.1",
    "terser-webpack-plugin": "^5.3.9",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "prettier": "^3.0.3",
    "tsx": "^4.1.4",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6",
    "fs-extra": "^11.1.1",
    "rimraf": "^5.0.5",
    "bundlesize": "^0.18.1",
    "http-server": "^14.1.1",
    "semantic-release": "^22.0.12",
    "benchmark": "^2.1.4",
    "clinic": "^12.1.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.spec.ts",
      "!src/**/*.test.ts",
      "!src/**/__tests__/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}